target Python

import FrostBase from "FrostBase.lf"

reactor FrostReactor(data_model_path = "", width = 1) extends FrostBase{
    input[width]  channel_in
    output[width] channel_out

    state data_model
    state protocol_mng

    reaction(startup){=
        """Initialize the reactor with the provided model path."""
        self.data_model = DataModelBuilder().get_data_model(self.data_model_path)
        self.protocol_mng = FrostProtocolMng(self.data_model)
    =}

}